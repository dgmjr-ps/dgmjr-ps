<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>latest</LangVersion>
    <OutputType>Library</OutputType>
    <OutputPath>bin/</OutputPath>
    <DestinationFolder>bin/</DestinationFolder>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MSBuild.Usings" />
    <PackageReference Include="Dgmjr.MSBuild.Extensions" />
    <PackageReference Include="Dgmjr.System.Extensions" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="./Sdk/*.*" PackagePath="Sdk/%(Filename)%(Extension)" />
  </ItemGroup>
</Project>
